# Supabase Auth OAuth credentials for local `supabase start`
# Copy to supabase/.env and replace placeholders.

SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=YOUR_GOOGLE_SECRET

SUPABASE_AUTH_EXTERNAL_AZURE_CLIENT_ID=YOUR_AZURE_CLIENT_ID
SUPABASE_AUTH_EXTERNAL_AZURE_SECRET=YOUR_AZURE_CLIENT_SECRET
# Azure authority URL, e.g. https://login.microsoftonline.com/common
# or tenant-specific https://login.microsoftonline.com/<tenant-id>
SUPABASE_AUTH_EXTERNAL_AZURE_URL=YOUR_AZURE_AUTHORITY_URL

SUPABASE_AUTH_EXTERNAL_FACEBOOK_CLIENT_ID=YOUR_FACEBOOK_APP_ID
SUPABASE_AUTH_EXTERNAL_FACEBOOK_SECRET=YOUR_FACEBOOK_APP_SECRET
